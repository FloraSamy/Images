			duration[0] = "7";
			
		
			banner[1] = "<a href='../admin/BannerServlet?c=BAN0000000619' target='_blank'><img src='banner/EXT0000000115/BAN0000000619.gif' border='0' height='87' class='banner-p' /></a>";
			duration[1] = "8";
			
		
				
	}
	
	function rotateBanner(){
		if(banner[i] == null){
			banner[i] = "";
			}
		document.getElementById("bnr_topRightBanner_rsdebar").innerHTML = banner[i];
		timeId = setTimeout("rotateBanner()", duration[i]*1000);
			if(i == banner.length - 1){
				i = 0;
			}else{
				i++;
			}
			if(isDefault){
				clearTimeout(timeId);
			}
		
		}
	
</script>
